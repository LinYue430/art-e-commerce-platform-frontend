  "devDependencies": {
    "@vue/component-compiler-utils": "^3.1.1",
    "autoprefixer": "^9.7.4",
    "gh-pages": "^2.2.0",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-static-files-copy": "^2.3.1",
    "vue-template-compiler": "^2.6.11"
  },
  "dependencies": {
    "faker": "^4.1.0",
    "lodash.debounce": "^4.0.8",
